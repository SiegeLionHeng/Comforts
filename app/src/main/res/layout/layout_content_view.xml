<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="HardcodedText,SpUsage">

    <data></data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">

        <!--我的内容-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableTop="@drawable/ic_pinglun"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center"
                android:paddingTop="@dimen/dp_10"
                android:paddingBottom="@dimen/dp_10"
                android:text="我的评论"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_12" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableTop="@drawable/ic_tiezi"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center"
                android:paddingTop="@dimen/dp_10"
                android:paddingBottom="@dimen/dp_10"
                android:text="我的帖子"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_12" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableTop="@drawable/ic_collect"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center"
                android:paddingTop="@dimen/dp_10"
                android:paddingBottom="@dimen/dp_10"
                android:text="我的收藏"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_12" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableTop="@drawable/ic_system_message"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center"
                android:paddingTop="@dimen/dp_10"
                android:paddingBottom="@dimen/dp_10"
                android:text="我的消息"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_12" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableTop="@drawable/ic_circle"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center"
                android:paddingTop="@dimen/dp_10"
                android:paddingBottom="@dimen/dp_10"
                android:text="我的圈子"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_12" />

        </LinearLayout>

        <ImageView
            android:id="@+id/iv_ad_position"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_10" />

        <!--创作中心-->
        <LinearLayout
            android:id="@+id/ll_creation_center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="@dimen/dp_15">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableStart="@drawable/ic_creation_icon"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center_vertical"
                android:text="创作中心"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_15" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_right_black_arrow" />

            <View
                android:layout_width="0.6dp"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/dp_15"
                android:background="@color/divider" />

            <TextView
                android:id="@+id/tv_contribute"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_15"
                android:background="@drawable/shape_original_create"
                android:drawableStart="@drawable/ic_upload"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center"
                android:paddingStart="@dimen/dp_10"
                android:paddingTop="@dimen/dp_3"
                android:paddingEnd="@dimen/dp_10"
                android:paddingBottom="@dimen/dp_3"
                android:text="原创投稿"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_12" />

        </LinearLayout>

        <View style="@style/style_divider" />

        <!--我的泡泡-->
        <LinearLayout
            android:id="@+id/ll_mine_bubble"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="@dimen/dp_15">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableStart="@drawable/ic_bubble"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center_vertical"
                android:text="我的泡泡"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_15" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_right_black_arrow" />


        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_10"
            android:background="@color/color_edf0f7" />

        <!--审核中心-->
        <LinearLayout
            android:id="@+id/ll_audit_center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="@dimen/dp_15">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableStart="@drawable/ic_audit"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center_vertical"
                android:text="审核中心"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_15" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp_10"
                android:text="审核状态"
                android:textColor="@color/color_ad"
                android:textSize="@dimen/dp_12" />


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_right_black_arrow" />


        </LinearLayout>

        <View style="@style/style_divider" />

        <!--浏览历史-->
        <LinearLayout
            android:id="@+id/ll_history"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="@dimen/dp_15">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableStart="@drawable/ic_history"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center_vertical"
                android:text="浏览历史"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_15" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_right_black_arrow" />


        </LinearLayout>

        <View style="@style/style_divider" />

        <!--推荐服务-->
        <LinearLayout
            android:id="@+id/ll_recommend_service"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="@dimen/dp_15">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableStart="@drawable/ic_recommend"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center_vertical"
                android:text="推荐服务"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_15" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp_10"
                android:text="精选服务任你选择"
                android:textColor="@color/color_ad"
                android:textSize="@dimen/dp_12" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_right_black_arrow" />


        </LinearLayout>

        <View style="@style/style_divider" />

        <!--勋章-->
        <LinearLayout
            android:id="@+id/ll_medal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="@dimen/dp_15">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableStart="@drawable/ic_medal"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center_vertical"
                android:text="我的勋章"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_15" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp_10"
                android:text="勋章等级"
                android:textColor="@color/color_ad"
                android:textSize="@dimen/dp_12" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_right_black_arrow" />


        </LinearLayout>

        <View style="@style/style_divider" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_10"
            android:background="@color/color_edf0f7" />

        <!--青少年模式-->
        <LinearLayout
            android:id="@+id/ll_young"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="@dimen/dp_15">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableStart="@drawable/ic_shield"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center_vertical"
                android:text="青少年模式"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_15" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp_10"
                android:text="未开启"
                android:textColor="@color/color_ad"
                android:textSize="@dimen/dp_12" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_right_black_arrow" />


        </LinearLayout>


        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_10"
            android:background="@color/color_edf0f7" />
        <!--视频壁纸-->
        <LinearLayout
            android:id="@+id/ll_video"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="@dimen/dp_15">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableStart="@drawable/ic_video"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center_vertical"
                android:text="视频壁纸"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_15" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_right_black_arrow" />


        </LinearLayout>

        <View style="@style/style_divider" />

        <!--帮助与反馈-->
        <LinearLayout
            android:id="@+id/ll_help_feed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="@dimen/dp_15">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableStart="@drawable/ic_help_feed"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center_vertical"
                android:text="帮助与反馈"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_15" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_right_black_arrow" />


        </LinearLayout>

        <View style="@style/style_divider" />


        <!--推荐给好友-->
        <LinearLayout
            android:id="@+id/ll_share"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="@dimen/dp_15">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableStart="@drawable/ic_shared"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center_vertical"
                android:text="推荐给好友"
                android:textColor="@color/black_text"
                android:textSize="@dimen/dp_15" />


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_right_black_arrow" />

        </LinearLayout>

    </LinearLayout>
</layout>